<launch>

  <arg name="fname" default="feeding_hrl"/>

  
  <node pkg="rosbag" type="record" name="rosbag" args="record -o $(find hrl_manipulation_task)/data/$(arg fname) 
                                                       /tf
                                                       /joint_states
                                                       /haptic_mpc
                                                       /right/haptic_mpc
                                                       /head_traj_controller/state
                                                       /hrl_manipulation_task
                                                       /netft_data
                                                       /manipulation_task
                                                       /feeding
                                                       pr2_fabric_l_forearm_sensor
                                                       pr2_fabric_l_upperarm_sensor
                                                       pr2_fabric_r_forearm_sensor
                                                       /SR300
                                                       /SR300/rgb/image_raw
                                                       /SR300/camera/depth_registered/image
                                                       /SR300/rgb/camera_info
                                                       /SR300/camera/depth_registered/camera_info
                                                       pr2_fabric_r_upperarm_sensor
                                                       /head_mount_kinect/hd/points
                                                       /head_mount_kinect/hd/camera_info
                                                       " output="screen"/>
</launch>



