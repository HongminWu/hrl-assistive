(define (domain pick_and_place_right)
 (:requirements :typing :conditional-effects)
 (:types 
  gripper item - physob
  location
 )

 (:constants hand - gripper
             target - item
             pick_loc - location
             place_loc - location
             hand_start_loc - location
             elsewhere - location
 )

 (:predicates
  (known ?l - location)
  (grasping ?g - gripper ?o - item)
  (at ?o - physob ?l - location)
 )

 (:action move-arm
  :parameters (?g - gripper ?l - location)
  :precondition (and (known ?l))
  :effect (and (forall (?loc - location)(not (at ?g ?loc)) )
               (at ?g ?l)
           )
  )

 (:action grab
  :parameters (?g - gripper ?o - item ?l - location)
  :precondition (and (known ?l)
                     (at ?o ?l)
                     (at ?g ?l)
                     (forall (?obj - item) (not (grasping ?g ?obj)))
                     (forall (?gr - gripper) (not (grasping ?gr ?o)))
                )
  :effect (and (grasping ?g ?o)
               (forall (?loc - location)
                 (not (at ?o ?loc))
               )
           )
 )

 (:action release
  :parameters (?g - gripper ?o - item ?l - location)
  :precondition (and (known ?l)
                     (at ?g ?l)
                     (grasping ?g ?o)
                     (forall (?obj - item) (not (at ?obj ?l)))
                 )
  :effect (and (at ?o ?l)
               (not (grasping ?g ?o))
          )
 )

 (:action id-location
  :parameters (?l - location)
  :effect (known ?l)
  )

 (:action forget-location
  :parameters (?l - location)
  :precondition (and (known ?l))
  :effect (not (known ?l))
  )
)
