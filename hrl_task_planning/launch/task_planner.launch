<launch>
    <node name="task_planner" pkg="hrl_task_planning" type="PyFF" output="screen" args="$(find hrl_task_planning)/src/ff">
        <param name="shaving/domain_file" value="$(find hrl_task_planning)/params/shaving.domain"/>
        <rosparam command="load" file="$(find hrl_task_planning)/params/shaving.yaml" ns="shaving"/>
    </node>
</launch>
