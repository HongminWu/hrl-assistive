  <node pkg="netft_rdt_driver" name="ft7" type="netft_node"
        args="192.168.0.123 --rate 100" respawn="false">
    <remap from='netft_data' to='/force_torque_pr2_sleeve' />
  </node>
  
  <arg name="marker_size" default="14.5" />
    <!--<arg name="marker_size" default="13.3" />-->
    <arg name="max_new_marker_error" default="0.05" />
    <arg name="max_track_error" default="0.1" />

    <arg name="cam_image_topic" default="/head_mount_kinect/qhd/image_color" />
    <arg name="cam_image_topic_points" default="/head_mount_kinect/qhd/points" />
