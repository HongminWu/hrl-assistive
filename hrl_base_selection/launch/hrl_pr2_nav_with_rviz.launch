<launch>
  <node pkg="rviz" type="rviz" name="$(anon rviz)" args="-d $(find hrl_base_selection)/rviz/move_base.rviz">
  </node>
  <node name="map_server" pkg="map_server" type="map_server" args="$(find hrl_base_selection)/data/maps/foliage_rm_test.yaml"/>


  <!--<include file="$(find pr2_navigation_perception)/lasers_and_filters.xml"/>-->
  <!--<include file="$(find pr2_navigation_perception)/ground_plane.xml"/>-->
  <include file="$(find hrl_base_selection)/launch/move_base.xml"/>
  <include file="$(find hrl_base_selection)/launch/amcl_node.xml"/>
  <node pkg="costmap_2d" type="costmap_2d_markers" name="voxel_visualizer">
    <remap from="voxel_grid" to="move_base_node/local_costmap/voxel_grid_throttled"/>
  </node>
</launch>
